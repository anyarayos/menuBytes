<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayoutwings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".CartEdit_WingProduct_Fragment">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="15dp"
        android:text="Select Quantity"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_garlic" />

    <TextView
        android:id="@+id/mealTotalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:text="0.00"
        android:textColor="#FFC61D25"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/btnUpdateOrder"
        app:layout_constraintStart_toEndOf="@+id/addQtyButton"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_garlic" />

    <TextView
        android:id="@+id/garlicSaucePrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:text="10.00"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheesePrice" />

    <Button
        android:id="@+id/backToCartBtn"
        android:layout_width="170dp"
        android:layout_height="46dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="25dp"
        android:text="Go Back to Cart"
        android:backgroundTint="#F0F0F0"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cheesePrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:text="10.00"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/javaRicePrice" />

    <TextView
        android:id="@+id/javaRicePrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="25dp"
        android:text="15.00"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtOption" />

    <CheckBox
        android:id="@+id/checkBox_garlic"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="Garlic Sauce"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_cheese" />

    <TextView
        android:id="@+id/txtOption"
        android:layout_width="204dp"
        android:layout_height="18dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="74dp"
        android:text="Optional"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtChoose"
        app:layout_constraintTop_toBottomOf="@+id/category_chip_group" />

    <TextView
        android:id="@+id/txtChoose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="11dp"
        android:text="Choose Add Ons"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/category_chip_group" />

    <TextView
        android:id="@+id/qtyText"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginTop="28dp"
        android:text="1"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/addQtyButton"
        app:layout_constraintStart_toEndOf="@+id/minusQtyButton"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/txtItemTitle"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:text="Item Name"
        app:layout_constraintStart_toEndOf="@+id/imgViewItemMenu"
        app:layout_constraintTop_toBottomOf="@+id/backToCartBtn" />

    <TextView
        android:id="@+id/txtItemDescription"
        android:layout_width="231dp"
        android:layout_height="66dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="Descriptiom"
        app:layout_constraintStart_toEndOf="@+id/imgViewItemMenu"
        app:layout_constraintTop_toBottomOf="@+id/txtItemTitle" />

    <ImageView
        android:id="@+id/imgViewItemMenu"
        android:layout_width="111dp"
        android:layout_height="99dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/menubyteslogo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backToCartBtn" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:text="Choose Flavors"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtItemDescription" />

    <TextView
        android:id="@+id/txtPick"
        android:layout_width="204dp"
        android:layout_height="18dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="15dp"
        android:text="Pick 1"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/txtItemDescription" />

    <CheckBox
        android:id="@+id/checkBox_java"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="Java Rice"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtChoose" />

    <CheckBox
        android:id="@+id/checkBox_cheese"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="Cheese Sauce"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_java" />

    <Button
        android:id="@+id/btnUpdateOrder"
        android:layout_width="117dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="25dp"
        android:text="Update Order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/minusQtyButton"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="43dp"
        android:layout_height="60dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:text="-"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/addQtyButton"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="43dp"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="5dp"
        android:text="+"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/minusQtyButton"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/category_chip_group"
        android:layout_width="320dp"
        android:layout_height="125dp"
        android:layout_marginStart="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:selectionRequired="true">

        <com.google.android.material.chip.Chip
            android:id="@+id/garlicPar"
            style="@style/Widget.MaterialComponents.Chip.Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Garlic Parmesan"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="282dp" />

        <com.google.android.material.chip.Chip
            android:id="@+id/saltedEgg"
            style="@style/Widget.MaterialComponents.Chip.Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Salted Egg"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="330dp" />

        <com.google.android.material.chip.Chip
            android:id="@+id/buffaloFlav"
            style="@style/Widget.MaterialComponents.Chip.Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Buffalo"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="282dp" />

        <com.google.android.material.chip.Chip
            android:id="@+id/bulgogiFlav"
            style="@style/Widget.MaterialComponents.Chip.Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkable="true"
            android:clickable="true"
            android:text="Bulgogi"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="330dp" />

        <com.google.android.material.chip.Chip
            android:id="@+id/soyGarlic"
            style="@style/Widget.MaterialComponents.Chip.Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Soy Garlic"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="282dp" />

        <com.google.android.material.chip.Chip
            android:id="@+id/sesameHoney"
            style="@style/Widget.MaterialComponents.Chip.Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sesame Honey"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="330dp" />
    </com.google.android.material.chip.ChipGroup>

</androidx.constraintlayout.widget.ConstraintLayout>